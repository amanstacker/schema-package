{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/faq-block",
  "version": "0.1.0",
  "title": "FAQ Block",
  "category": "widgets",
  "icon": "editor-help",
  "description": "A block to add FAQs with schema markup.",
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "textdomain": "faq-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "faqs": {
      "type": "array",
      "default": []
    },
    "boxStyle": {
      "type": "string",
      "default": "bordered"
    },
    "showNumbering": {
      "type": "boolean",
      "default": false
    },
    "headingLevel": {
      "type": "number",
      "default": 4
    },
    "listStyle": {
      "type": "string",
      "default": "decimal"
    }
  }
}